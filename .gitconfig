[remote "jonas"]
    url = git@github.com:jonas/tig.git
    fetch = +refs/heads/*:refs/remotes/jonas/*
[remote "origin"]
    url = git@github.com:huangyingw/jonas_tig.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = jonas
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/jonas/tig
